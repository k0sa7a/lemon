<div class="card">
  <% url = location.photos.attached? ? cl_image_path(location.photos.first.key) : "stock_location1.jpg" %>
  <div class="card-header ">
    <%= image_tag(url) %>
  </div>
  <div class="card-body">
    <div>
      <h2><%= location.title %></h2>
      <p><%= location.address%></p>
    </div>
  </div>
</div>
